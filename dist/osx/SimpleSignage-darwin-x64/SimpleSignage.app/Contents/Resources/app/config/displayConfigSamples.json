
/* Tests a single webview */
{
	"name": "test-webview",
	"screen": [{
		"module": {
			"type": "webview",
			"src": "http://www.sky.com"
		}
	}]
}

/* Tests a webview-carousel */
{
	"name": "test-webview-carousel",
	"screen": [{
		"module": {
			"type": "webview-carousel",
			"rotationTime": 5,
			"modules": [
				{
					"module": {
						"type": "webview",
						"src": "http://www.sky.com"
					}
				},
				{
					"module": {
						"type": "webview",
						"src": "http://news.sky.com/"
					}
				}
			]
		}
	}]
}

/* Tests a webview-grid */
{
	"name": "test-webview-grid",
	"screen": [{
		"module": {
			"type": "webview-grid",
			"rows": 2,
			"cols": 2,
			"modules": [
				{
					"module": {
						"type": "webview",
						"src": "http://news.sky.com"
					}
				},
				{
					"module": {
						"type": "webview",
						"src": "http://www.rt.com/"
					}
				},
				{
					"module": {
						"type": "webview",
						"src": "http://www.bbc.co.uk/news"
					}
				},
				{
					"module": {
						"type": "webview",
						"src": "http://edition.cnn.com/"
					}
				}
			]
		}
	}]
}
